{
  "compilerOptions": {
   "typeRoots": ["./node_modules/@types", "./types"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "jsx": "react-native",
    
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "types/**/*.ts", "src"
  ],
  "extends": "expo/tsconfig.base"
}
